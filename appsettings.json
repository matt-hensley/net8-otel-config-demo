{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "OpenTelemetry": "Debug"
        }
    },
    "AllowedHosts": "*",
    "OpenTelemetry": {
        "otlp": {
            "Endpoint": "https://your-otlp-gateway-url.grafana.net/otlp",
            "Protocol": "HttpProtobuf",
            "Headers": "Authorization=Basic your-access-token"
        }
    }
}
